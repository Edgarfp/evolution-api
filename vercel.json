{
  "version": 2,
  "builds": [
    {
      "src": "evolution-api/package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "cd evolution-api && npm install && npm run build",
        "includeFiles": ["evolution-api/manager/dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "evolution-api/dist/main.js"
    }
  ]
}
